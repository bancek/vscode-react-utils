{
    "name": "react-utils",
    "displayName": "React utils",
    "description": "React utils.",
    "version": "0.1.0",
    "publisher": "lukazakrajsek",
    "repository": {
        "type": "git",
        "url": "https://github.com/bancek/vscode-react-utils"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "https://raw.githubusercontent.com/bancek/vscode-react-utils/master/LICENSE.md"
        }
    ],
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "react",
        "utils",
        "jsx"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.useClassnames",
            "title": "React utils: Use classnames"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
